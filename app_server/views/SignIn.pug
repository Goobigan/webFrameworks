extends layout

block content

  .container.register-form
    form(role='form', action="/SignIn",method="post")
      .note
        h5.section-title.h1= title
      .form-content
        .row
          .col-sm-4
            .form-group
              input.form-control#username(type='text' placeholder='Username ' value='')
            .form-group
              input.form-control#password(type='text' placeholder='Password ' value='')
        button.btn.btn-default(type='submit') Submit

